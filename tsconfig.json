{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "moduleResolution": "Node"
  },
  "include": [
    "lib/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
